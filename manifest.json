{
  "manifest_version": 2,
  "name": "Facebook Active User Notifier",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://waterdelivery.localhost/*",
        "https://*.facebook.com/*",
        "https://facebook.com/*",
        "https://fb.com/*"
      ],
      "css": ["style.css"],
      "js": ["elements.js","content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
